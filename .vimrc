
execute pathogen#infect()

colo gruvbox
syntax enable

filetype on
filetype off
filetype plugin indent on

set autoindent
set autoread
set clipboard=unnamed
set encoding=utf-8
set expandtab
set softtabstop=2
set tabstop=8
set wildmenu
set ignorecase
set incsearch
set statusline=%F%m%r%<\ %=%l,%v\ [%L]\ %p%%
set laststatus=2
set list
set number relativenumber
set ruler
set scrolloff=4
set shiftwidth=2
set showcmd
set mouse=a


